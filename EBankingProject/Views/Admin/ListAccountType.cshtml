
@model IEnumerable<EBankingProject.Models.AccountType>

@{
    ViewBag.Title = "ListAccountType";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h4 class="mt-4">List of AccountType</h4>
<hr />
<p>
    @Html.ActionLink("Create New", "AddAccountType", "Admin")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TypeOfAccount)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TypeOfAccount)
            </td>

        </tr>
    }

</table>


